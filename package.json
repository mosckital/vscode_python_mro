{
	"name": "python-mro",
	"displayName": "Python MRO",
	"description": "To show the MRO (Method Resolution Order) of a Python class in VS Code.",
	"author": {
		"name": "Kaiyan"
	},
	"publisher": "kaiyan",
	"version": "0.0.3",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mosckital/vscode_python_mro"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [],
	"keywords": [
		"Python MRO Method Resolution Order"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./dist/extension",
	"contributes": {
		"configuration": {
			"title": "PythonMRO",
			"properties": {}
		},
		"commands": [
			{
				"command": "pythonMRO.showMRO",
				"title": "Show MRO list"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"test": "node ./client/out/test/runTest",
		"test_windows": "node .\\client\\out\\test\\runTest"
	},
	"dependencies": {
		"@types/vscode": "^1.46.0",
		"@types/get-port": "^4.2.0",
		"vscode-languageclient": "^6.1.3",
		"get-port": "^5.1.1"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/js-yaml": "^3.11.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"js-yaml": "^3.14.0",
		"mocha": "^7.1.2",
		"ts-loader": "^8.1.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.6.0"
	}
}
